  "devDependencies": {
    "@commitlint/cli": "^19.8.x",
    "@commitlint/config-conventional": "^19.8.x",
    "@eslint/js": "^9.17.x",
    "@faker-js/faker": "^9.3.x",
    "@testing-library/jest-dom": "^6.6.x",
    "@testing-library/react": "^16.3.x",
    "@testing-library/user-event": "^14.6.x",
    "@types/luxon": "^3.4.x",
    "@types/node": "^22.13.x",
    "@types/react": "^19.1.x",
    "@types/react-dom": "^19.1.x",
    "@types/styled-theming": "^2.2.x",
