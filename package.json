    "lint-staged": "^16.1.x",
    "postcss": "^8.5.x",
    "pre-commit": "^1.2.x",
    "standard-version": "^9.5.x",
    "tailwindcss": "^3.0.1x",
    "eslint": "^9.19.x",
    "eslint-plugin-prettier": "^5.2.x",
    "eslint-plugin-react": "^7.37.x",
    "eslint-plugin-react-hooks": "^5.2.x",
