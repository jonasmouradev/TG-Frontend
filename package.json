    "class-variance-authority": "^0.7.x",
    "clsx": "^2.1.x",
    "embla-carousel-react": "^8.5.x",
    "i18next": "^25.3.x",
    "js-cookie": "^3.0.x",
    "jwt-decode": "^4.0.x",
    "lucide-react": "^0.476.x",
    "luxon": "^3.5.x",
    "next-themes": "^0.4.x",
    "react": "^19.1.x",
    "react-dom": "^19.1.x",
    "react-hook-form": "^7.60.x",
    "react-i18next": "^15.6.x",
